{
 "version": 2,
 "name": "node-restapi",
 "builds": [
    { "src": "index.js", "use": "@vercel/node" }
 ],
 "routes": [
    { "src": "/(.*)",
     "headers": { "cache-control": "no-cache, no-store, must-revalidate", "Access-Control-Allow-Origin": "*", "Open-Source": "" },
     "dest": "index.js" }
 ]
}
